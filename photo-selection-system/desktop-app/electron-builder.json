  "win": {
    "target": [
      "nsis"
    ],
    "icon": "assets/icon.png"
  },
  "nsis": {
    "oneClick": false,
    "allowToChangeInstallationDirectory": true
  },
  "mac": {
    "target": [
      "dmg"
    ],
    "icon": "assets/icon.icns"
  },
  "linux": {
    "target": [
      "AppImage"
    ],
    "icon": "assets/icon.png",
    "category": "Graphics"
  }
